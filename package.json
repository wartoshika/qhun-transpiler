{
  "name": "@wartoshika/qhun-transpiler",
  "version": "1.0.0-alpha.4",
  "description": "A tool to transpiles a Typescript project into lua.",
  "main": "dist/api.js",
  "types": "dist/api.d.ts",
  "bin": "dist/bin/qhun-transpiler.js",
  "bugs": {
    "url": "https://github.com/wartoshika/qhun-transpiler/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wartoshika/qhun-transpiler"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha -r ts-node/register \"test/**/*.spec*.ts\"",
    "test-async": "./node_modules/.bin/mocha-parallel-tests -r ts-node/register \"test/**/*.spec*.ts\"",
    "test-seperate": "./node_modules/.bin/mocha -r ts-node/register \"test/**/*.spec.sep.ts\"",
    "test:coverage": "./node_modules/.bin/nyc ./node_modules/.bin/mocha -r ts-node/register \"test/**/*.spec.ts\"",
    "coverage:send:coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build-api": "./node_modules/.bin/webpack --config ./webpack.api.config.js",
    "build-declaration": "./node_modules/.bin/tsc -p tsconfig.declaration.json",
    "build-declaration:append-module-declaration": "./node_modules/.bin/replace-in-file \"/declare module .index./\" \"declare module '@wartoshika/qhun-transpiler'\" ./dist/api.d.ts --isRegex",
    "build-cli": "./node_modules/.bin/webpack --config ./webpack.cli.config.js",
    "build": "./node_modules/.bin/npm-run-all build-api build-cli build-declaration build-declaration:append-module-declaration",
    "run-cli": "./node_modules/.bin/ts-node src/cli/Cli.ts",
    "postversion": "git push --follow-tags"
  },
  "author": "wartoshika <dev@qhun.de>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.2.2",
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/md5": "^2.1.33",
    "@types/mkdirp": "^0.5.2",
    "@types/mocha": "^5.2.7",
    "@types/mock-fs": "^3.6.30",
    "@types/node": "^12.7.5",
    "chai": "^4.2.0",
    "copyfiles": "^2.1.1",
    "coveralls": "^3.0.6",
    "file-loader": "^4.2.0",
    "istanbul": "^0.4.5",
    "lua.vm.js": "0.0.1",
    "mocha": "^6.2.0",
    "mocha-parallel-tests": "^2.2.2",
    "mocha-typescript": "^1.1.17",
    "mock-fs": "^4.10.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "raw-loader": "^3.1.0",
    "replace-in-file": "^4.1.3",
    "source-map-support": "^0.5.13",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-merge": "^4.2.2"
  },
  "dependencies": {
    "command-line-args": "^5.1.1",
    "command-line-usage": "^6.0.2",
    "md5": "^2.2.1",
    "mkdirp": "^0.5.1",
    "node-watch": "0.6.3",
    "rxjs": "^6.5.3",
    "typescript": "~3.2",
    "typescript-mix": "^3.1.3"
  }
}
