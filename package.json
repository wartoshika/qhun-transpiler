{
  "name": "@wartoshika/qhun-transpiler",
  "version": "0.0.3",
  "description": "A command line tool to transpiles a Typescript project into supported other languages.",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/wartoshika/qhun-transpiler"
  },
  "scripts": {
    "test": "./node_modules/.bin/mocha -r ts-node/register test/**/*.spec.ts",
    "coverage": "./node_modules/.bin/nyc ./node_modules/.bin/mocha -r ts-node/register test/**/*.spec.ts",
    "build": "./node_modules/.bin/tsc",
    "run-ts": "./node_modules/.bin/ts-node src/index.ts",
    "bundle": "./node_modules/.bin/rollup -c ./rollup.config.js",
    "postversion": "git push --follow-tags"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  },
  "author": "wartoshika <dev@qhun.de>",
  "license": "MIT",
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/command-line-args": "^5.0.0",
    "@types/command-line-usage": "^5.0.1",
    "@types/glob": "^5.0.35",
    "@types/mocha": "^5.2.4",
    "@types/mock-fs": "^3.6.30",
    "@types/node": "^10.5.2",
    "@types/shelljs": "^0.8.0",
    "@types/tmp": "0.0.33",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-typescript": "^1.1.17",
    "mock-fs": "^4.6.0",
    "npm-run-all": "^4.1.3",
    "nyc": "^12.0.2",
    "rollup": "^0.64.1",
    "rollup-plugin-commonjs": "^9.1.5",
    "rollup-plugin-node-resolve": "^3.3.0",
    "source-map-support": "^0.5.6",
    "ts-node": "^7.0.0",
    "tslint": "^5.11.0"
  },
  "dependencies": {
    "command-line-args": "^5.0.2",
    "command-line-usage": "^5.0.5",
    "shelljs": "^0.8.2",
    "typescript": "^2.9.2",
    "typescript-mix": "^3.1.3",
    "uuid": "^3.3.2"
  }
}
